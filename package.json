{
    "name": "paypal-sdk-e2e-tests",
    "version": "0.1.0",
    "description": "Automation tests for the PayPal JS SDK",
    "scripts": {
        "build": "tsc -p .",
        "prepare": "husky install",
        "lint": "eslint --ext .js,.ts .",
        "format": "prettier --write --ignore-unknown .",
        "docs": "serve docs",
        "test": "wdio resources/config/wdio.local-runner.conf.ts",
        "test-browserstack": "wdio resources/config/wdio.browserstack.conf.ts",
        "test-browserstack-german": "wdio resources/config/wdio.browserstack.german.conf.ts",
        "generateMochawesome": "marge mocha-report/wdio-ma-merged.json --reportTitle 'PayPal SDK Tests'",
        "save-credentials": "ts-node resources/utils/save-credentials.ts"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "@paypal/sdk-constants": "^1.0.107",
        "@wdio/cli": "^7.6.1",
        "@wdio/local-runner": "^7.6.1",
        "@wdio/mocha-framework": "^7.6.0",
        "browserstack-local": "^1.4.8",
        "chai": "^4.3.4",
        "dotenv": "^10.0.0",
        "minimist": "^1.2.5",
        "mochawesome-report-generator": "^3.1.5",
        "node-fetch": "^2.6.6",
        "webdriverio": "^7.6.1"
    },
    "devDependencies": {
        "@types/chai": "^4.2.18",
        "@types/minimist": "^1.2.1",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.17.0",
        "@types/node-fetch": "^2.5.12",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "serve": "^12.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.2",
        "wdio-mochawesome-reporter": "^4.0.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --ext .js,.ts"
        ],
        "*.{js,jsx,ts,tsx,html,css,json,md,yaml,yml}": [
            "prettier --write --ignore-unknown"
        ]
    }
}
